{
  "manifest_version": 2,

  "name": "bdloader",
  "description": "Balhau downloader chrome extension",
  "version": "1.0.10",

  "options_page": "options.html",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title" : "Balhau resource downloader"
  },
  "icons": {
          "16": "icon.png",
          "48": "icon.png",
          "128": "icon.png"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://pi.balhau.net/",
    "storage"
  ]
}
