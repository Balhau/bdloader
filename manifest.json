{
  "manifest_version": 2,

  "name": "bdloader",
  "description": "Balhau downloader chrome extension",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title" : "Balhau resource downloader"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
    "http://pi.balhau.net"
  ]
}
